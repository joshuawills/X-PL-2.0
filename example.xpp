extern printf(i8*, ...) i32;
extern exit(i64) void;

fn increment_one(mut x: i64) i64 {
	x = x + 1;
	return x;
}

fn main() void {

	let y: i8 = 'a';

    let mut x = 1;
	printf("my favourite number is: %d\n", x);

	while x < 10 {
		printf("%d\n", x);
		x = x + 1;
	}

	if y == 'a' {
		printf("wow");
	} else {
		printf("sad");
	}
}

