// Messing around with break and continue

extern printf(i8*, ...) i32;

fn main() void {

    let mut i: i64 = 0;
    while i < 10 {
        printf("%d\n", i);
        i += 1;
    }

    let mut j: i64 = -1;
    while j < 10 {
        if j == 5 {
            break;
        }
        j += 1;
        if j == 2 {
            continue;
        }
        printf("%d\n", j);
    }

    loop i {
        if i == 1 {
            continue;
        }
        printf("%d\n", i);
        if i == 3 {
            break;
        }
    }

    loop haha in 3, 5 {
        printf("%d\n", haha);
    }

    loop l_one in 5 {
        
        if l_one == 1 {
            break;
        }
        
        loop l_two in 3 {
            if l_two == 1 {
                continue;
            }
            printf("%d\n", l_one);
            printf("%d\n", l_two);
        }
    }

}