// Boolean short_circuiting

extern printf(i8*, ...) i32;

fn get_true() bool {
    printf("side effect\n");
    return true;
}

fn main() void {
    printf("A\n");
    let x = true || get_true();
    printf("B\n");
    let y = false || get_true();
    printf("C\n");
    let a = true && get_true();
    printf("D\n");
    let b = false && get_true();
}